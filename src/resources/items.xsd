<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="lines" type="linesType"/>
  <xs:complexType name="linesType">
    <xs:sequence>
      <xs:element type="itemType" name="item" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="itemType">
    <xs:all>
      <xs:element type="xs:string" name="shortName"/>
      <xs:element type="xs:string" name="longName"/>
      <xs:element type="xs:integer" name="stack"/>
      <xs:element type="xs:string" name="type"/>
    </xs:all>
    <xs:attribute type="xs:string" name="id" use="required"/>
    <xs:attribute type="xs:string" name="visible" />
  </xs:complexType>
</xs:schema>